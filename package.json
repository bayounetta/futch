{
  "name": "futch.dev",
  "private": true,
  "description": "a place to keep links to my work and all my cute things",
  "repository": "https://github.com/metalcanine/futch/",
  "version": "0.1.0",
  "license": "UNLICENSED",
  "scripts": {
    "watch:sass": "sass --watch resources/styles:build/css",
    "build:sass": "sass resources/styles:build/css",
    "watch:eleventy": "eleventy --serve",
    "build:eleventy": "eleventy",
    "start": "npm-run-all --parallel watch:*",
    "build": "npm-run-all build:*",
    "clean": "rm -rf ./build ./node_modules ./.cache",
    "lint": "prettier --check './**/*.{js,json,html,scss,md}'",
    "test": "",
    "deploy": "env NODE_ENV=production npm run build"
  },
  "dependencies": {
    "@11ty/eleventy": "^1.0.0",
    "@11ty/eleventy-img": "^0.10.0",
    "@11ty/eleventy-plugin-syntaxhighlight": "^3.1.3",
    "gh-pages": "^2.2.0",
    "markdown-it": "^12.2.0",
    "markdown-it-footnote": "^3.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "sass": "^1.43.2",
    "snyk": "^1.434.2"
  }
}
